be N the later used name

#include <iostream>

N(x)

 =>  std::cout << "D: x: " << x << std::endl;

NFL(x)

 =>  std::cout << "D: x: " << x << " (in file: " << __FILE__ << " line: " << __LINE__ << std::endl;

NF => only file name

NL => only line number





